<?xml version="1.0"?>
<TopologyBatch>

  <vtkSQLog/>

  <vtkSQBOVMetaReader
      block_size="96 96 96"
      periodic_bc="0 0 0">
      <arrays> b </arrays>
  </vtkSQBOVMetaReader>

  <vtkSQHemisphereSource
      center="511 511 511"
      radius="32"
      resolution="32"
      north="0 1 0"/>

  <vtkSQPlaneSource
      origin="200 511 0"
      point1="1023 511 0"
