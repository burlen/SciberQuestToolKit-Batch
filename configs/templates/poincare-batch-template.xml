<?xml version="1.0"?>
<PoincareBatch>

  <vtkSQLog/>

  <vtkSQBOVMetaReader
      block_size="4 4 4"
      periodic_bc="1 1 1">
       <arrays> b </arrays>
  </vtkSQBOVMetaReader>

  <vtkSQPlaneSource
      origin="-3 0 -3"
      point1="3 0 -3"
      point2="-3 0 3"
      resolution="4 4"/>

  <vtkSQFieldTracer
      integrator_type="2"
      max_arc_length="1000"
      max_step_size="0.1"
      null_threshold="0.001"
      dynamic_scheduler="0"/>

</PoincareBatch>
