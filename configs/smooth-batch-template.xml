<?xml version="1.0"?>
<SmoothBatch>

  <vtkSQBOVReader
      cb_enable="1"
      ISubset="-1 -1"
      JSubset="-1 -1"
      KSubset="-1 -1">
       <arrays> ue </arrays>
  </vtkSQBOVReader>

  <vtkSQKernelConvolution
      stencil_width="13"
      compute_residual="0"
      kernel_type="0"
      cpu_driver_optimization="1"/>
   <!--
   ==================
   general
   ==================
   kernel types: Gaussian=0 Log Of Gaussian=1 Constant=2

   ==================
   cuda specific
   ==================
   gpu memory types: global=0 constant=1 texture=2

   numberofmpirankstousecuda="0"
   numberofactivecudadevices="0"
   numberofwarpspercudablock="8"
   inputcudamemorytype="2"
   kernelcudamemorytype="0"
   -->

  <vtkSQBOVWriter
      cb_enable="1"
      cb_buffer_size="1073741824"
      stripe_count="160"/>

</SmoothBatch>
